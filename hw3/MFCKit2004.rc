        MENUITEM "Show &Osculating Sphere",     ID_FRENET_SHOW_OSC_SPHERE
        MENUITEM SEPARATOR
        MENUITEM "Draw &Evolute",               ID_FRENET_DRAWEVOLUTE
        MENUITEM "Draw Off&set",                ID_FRENET_DRAWOFFSET
        MENUITEM SEPARATOR
        POPUP "Animation"
        BEGIN
            MENUITEM "&Start",                      ID_ANIMATION_START
            MENUITEM "S&top",                       ID_ANIMATION_STOP
        END
        MENUITEM SEPARATOR
        MENUITEM "Properties",                  ID_FRENET_PROPERTIES
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About MFCKit2004...",        ID_APP_ABOUT
    END
END

