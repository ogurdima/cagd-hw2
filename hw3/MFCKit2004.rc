        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
    END
    POPUP "&Options"
    BEGIN
        POPUP "Fuzziness"
        BEGIN
            MENUITEM "&More",                       ID_FUZZINESS_MORE
            MENUITEM "&Less",                       ID_FUZZINESS_LESS
        END
        MENUITEM "&Reset",                      ID_OPTIONS_RESET
        MENUITEM SEPARATOR
        POPUP "&Mouse Sensitivity"
        BEGIN
            POPUP "&Rotation"
            BEGIN
                MENUITEM "&More",                       ID_ROTATION_MORE
                MENUITEM "&Less",                       ID_ROTATION_LESS
            END
            POPUP "&Translation"
            BEGIN
                MENUITEM "&More",                       ID_TRANSLATION_MORE
                MENUITEM "&Less",                       ID_TRANSLATION_LESS
            END
            POPUP "&Scaling"
            BEGIN
                MENUITEM "&More",                       ID_SCALING_MORE
                MENUITEM "&Less",                       ID_SCALING_LESS
            END
        END
    END
    POPUP "Frenet"
    BEGIN
        MENUITEM "Show &Axes",                  ID_FRENET_SHOWAXES
        MENUITEM SEPARATOR
        MENUITEM "Show &Frenet Frame",          ID_FRENET_SHOWFRENETFRAME
        MENUITEM "Show &Curvature",             ID_FRENET_SHOWCURVATURE
